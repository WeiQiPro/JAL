struct Point {
    i32 x;
    i32 y;

    Point new: fn(x: i32, y: i32);

    Modify
        move: fn(self, dx: i32, dy: i32);
}

Point.new(x, y) {
    return Point { x, y };
}

Point.move(dx: i32, dy: i32) {
    self.x += dx;
    self.y += dy;
}

fn main() {
    let p1 := Point.new(10, 20);
    p1.move(5, -3);

    let a := 15;
    let b := 25;
    let p2: Point = { a, b };
    p2.move(-3, 7);
}
